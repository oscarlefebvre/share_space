<div class="container">
  <div class="row">
    <div class="col-xs-12 col-sm-8">
      <div class="searchbar">
        <div class="well">
          Filter by price interval:
          <b>€ 10</b>
          <div class="slider slider-horizontal" style="width: 414px;">
            <div class="slider-track">
              <div class="slider-selection" style="left: 24.2424%; width: 20.202%;"></div>
              <div class="slider-handle round" style="left: 24.2424%;"></div>
            </div>
            <div class="tooltip top" style="top: -40px; left: 111.182px;">
              <div class="tooltip-arrow"></div>
              <div class="tooltip-inner">250 : 450</div>
            </div><input type="text" class="span2" value="" data-slider-min="10" data-slider-max="1000" data-slider-step="5" data-slider-value="[250,450]" id="sl2"></div>
            <b>€ 1000</b>
        </div>
      </div>
      <div class="jesaispas">
        <input type="text" class="datepicker update-link" name="checkin" value="<%= @reservation_attributes[:start_date] %>" placeholder: "Check-in">
      </div>
      <div class="search-results">
        <% @spaces.each do |space| %>
          <div class="col-xs-12 col-sm-4">
          <%= render "card", space: space %>
          </div>
        <% end %>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-12 col-sm-4">
      <div id="map" style="width: 800px; height: 600px;"></div>
    </div>
  </div>
</div>



<!-- app/views/reservation/new.html.erb -->
<h1 class="text-center"> Add a reservation</h1>

<div class="container">
 <div class="row">
    <%= form_for [@space, @reservation] do |f| %>
    <div class="col-xs-12 col-sm-4">
      <h2><%= f.date_field :start_date, placeholder: "test" %></h2>
      <h2><%= f.date_field :end_date %></h2>
      <%= f.submit "add a review" %>
   </div>
   <% end %>
 </div>
</div>
